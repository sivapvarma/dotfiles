! Do not edit .Xresources but .Xresources.def
! Rebuild with cpp -P .Xresources.def .Xresources
! xrdb -merge .Xresources


! Xft settings
Xft.dpi: 96
Xft.lcdfilter:  lcddefault
Xft.antialias:  true
Xft.autohint:   true
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.rgba:       rgb

! Use TrueType font and size by default
XTerm*faceName: SourceCodePro
XTerm*faceSize: 12


